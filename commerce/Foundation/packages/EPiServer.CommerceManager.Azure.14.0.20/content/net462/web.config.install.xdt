<?xml version="1.0" encoding="utf-8"?>
<configuration xmlns:xdt="http://schemas.microsoft.com/XML-Document-Transform">
	<appSettings>
		<add key="Microsoft.ServiceBus.ConnectionString" xdt:Transform="Remove" xdt:Locator="Match(key)" />
	</appSettings>
	<system.webServer>
		<staticContent xdt:Transform="InsertIfMissing" />
	</system.webServer>
	<system.webServer>
		<staticContent>
			<!-- Need remove elements to remove mime types from IIS in local before changing mime types.-->
			<remove fileExtension=".woff" xdt:Transform="InsertIfMissing" xdt:Locator="Match(fileExtension)"/>
			<remove fileExtension=".ttf" xdt:Transform="InsertIfMissing" xdt:Locator="Match(fileExtension)"/>
			<remove fileExtension=".svg" xdt:Transform="InsertIfMissing" xdt:Locator="Match(fileExtension)"/>
			<remove fileExtension=".eot" xdt:Transform="InsertIfMissing" xdt:Locator="Match(fileExtension)"/>
			<mimeMap fileExtension=".woff" mimeType="application/font-woff" xdt:Transform="InsertIfMissing" xdt:Locator="Match(fileExtension)"/>
			<mimeMap fileExtension=".ttf" mimeType="application/octet-stream" xdt:Transform="InsertIfMissing" xdt:Locator="Match(fileExtension)"/>
			<mimeMap fileExtension=".svg" mimeType="image/svg+xml" xdt:Transform="InsertIfMissing" xdt:Locator="Match(fileExtension)"/>
			<mimeMap fileExtension=".eot" mimeType="application/vnd.ms-fontobject" xdt:Transform="InsertIfMissing" xdt:Locator="Match(fileExtension)"/>
		</staticContent>
	</system.webServer>
</configuration>
